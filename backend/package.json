{
  "name": "expense-tracker-backend",
  "version": "1.0.0",
  "description": "Enterprise-grade expense management backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "worker": "node worker.js",
    "db:init": "node database/init.js",
    "db:migrate": "npx knex migrate:latest --knexfile knexfile.js",
    "db:rollback": "npx knex migrate:rollback --knexfile knexfile.js",
    "db:seed": "npx knex seed:run --knexfile knexfile.js",
    "db:seed:legacy": "node database/seeders/defaultCategories.js",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "expense-tracker",
    "financial-analytics",
    "budget-management",
    "rest-api",
    "nodejs",
    "express"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bullmq": "^5.68.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "natural": "^8.1.0",
    "pg": "^8.18.0",
    "python-shell": "^5.0.0",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "sqlite3": "^5.1.6",
    "uuid": "^13.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}